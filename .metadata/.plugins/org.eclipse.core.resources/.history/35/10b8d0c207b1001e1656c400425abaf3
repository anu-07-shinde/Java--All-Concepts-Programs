package Demo;

public interface Demo {
	
	
	final int a = 20;
	
	 void display();

}
class DisplayMsg implements Demo
{
	public void display()
	{
		System.out.println("Hello Anuradha!..........");
	}
	



	public static void main(String[] args) {
		
		DisplayMsg d = new DisplayMsg();
		d.display();
		System.out.println(a);
		
	}
}
