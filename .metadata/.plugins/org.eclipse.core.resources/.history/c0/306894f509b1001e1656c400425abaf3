package RealExample;

class Bicycle implements Vehicle
{
	int speed;
	int gear;
	@Override
	public void gearChange(int gear) {
		
		this.gear=gear;
		
		
	}

	@Override
	public void speedUp(int increment) {

		speed = speed + increment;
		
	}

	@Override
	public void breakDown(int decrement) {
		
		speed = speed - decrement;

		
	}
}
	
