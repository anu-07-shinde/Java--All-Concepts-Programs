package ArrayListStack;
import java.util.*;

public class Example {
	
	class Stack
	{
		
	ArrayList<Integer> list = new ArrayList<>();
		
		public void push(int data)
		{
			list.add(data);
		}
	
	}



	public static void main(String[] args) {
		
	
		Stack s = new Stack();
		s.push(1);
		s.push(2);
		s.push(3);
		s.push(4);
		
		System.out.println(s);
		
	}

}
}
